<!DOCTYPE html>
<html>
<head>
	<meta charset= "utf-8">
	<title>Login</title>
	<% include partials/head %>
</head>
<body class="login">
	<div class="container">
		<div class="col-md-5 mx-auto">
			<div class="col -md-5 mt-5 mx-auto" style="background: white;">
					<div class="row" >
						<div class="col-md-12 md-center" >
							<h1 class="text-center mb-4" >
								<span class="fas fa-user">
								</span>
								Login
							</h1>
						</div>
					</div>
					<form action="" method="post">
						<div class="form-group">
							<div class="col-md-12">
								<div>
									<h5 for="user" class="text-center">Uneg user</h5>
								</div>
								<div class="col-md-8 mx-auto">
									<input type="user" name="user" value="" class="form-control">
								</div>
							</div>
							<div class="col-md-12">
								<div>
									<h5 for="password" class="text-center">Password</h5>
								</div>
								<div class="col-md-8 mx-auto">
									<input type="password" name="password" value="" class="form-control">
								</div>
							</div>
						</div>
						<div class="col-md-4 mx-auto">
							<input type="submit" href="/sistema" class="btn btn-dark btn-lg">
						</div>
						</div>
					</form>
			</div>
		</div>
	</div>
</body>
</html>